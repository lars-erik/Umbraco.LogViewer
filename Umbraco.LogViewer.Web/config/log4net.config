<?xml version="1.0"?>
<log4net>
  <root>
    <priority value="Info"/>
    <appender-ref ref="SignalRAppender"/>
  </root>

  <appender name="SignalRAppender" type="Umbraco.LogViewer.SignalRAppender, Umbraco.LogViewer">
    <layout type="log4net.Layout.PatternLayout">
      <conversionPattern value="%date [%thread] %-5level %logger - %message%newline" />
    </layout>
  </appender>
</log4net>
